OAAZ,EAAoB,IAApB;AACH,iBALK,EAKH,KALG,CAKG,UAAA,GAAE,EAAK;AACZ,kBAAA,MAAI,CAAC,MAAL,CAAY,OAAZ,CAAoB,gCAApB;;AACA;AACH,iBARK,CAPU;;AAAA;AAAA;AAAA,uBAgBC,MAAI,CAAC,UAAL,EAhBD;;AAAA;AAgBV,gBAAA,EAhBU;AAiBhB,gBAAA,MAAI,CAAC,IAAL,GAAY,EAAZ;;AAjBgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkBnB,KAzDI;AA0DL,IAAA,SA1DK,qBA0DK,GA1DL,EA0DU;AACX,WAAK,cAAL,GAAsB,KAAtB;AACA,WAAK,SAAL,GAAiB,GAAjB;AACH,KA7DI;AA8DL,IAAA,iBA9DK,6BA8Da,MA9Db,EA8DqB;AACtB,WAAK,WAAL,GAAmB,MAAnB,CADsB,CAEtB;;AACA,WAAK,KAAL,CAAW,IAAX,CAAgB,KAAhB,GAAwB,KAAK,WAA7B;AACJ;AAlEK;AAnCE,CAAf","sourcesContent":["<template>\n    <div class=\"view_app\">\n        <div class=\"app_list\">\n            <div class=\"add_new_app d-flex justify-center align-center\">\n                <button @click=\"modeCreateApp()\" class=\"btn btn-normal\">Create new App <span class=\"icon icon-plus\"></span></button>\n            </div>\n            <hr>\n            <div class=\"app_name_list\">\n                <p class=\"info-text\" v-if=\"apps === null || apps.length === 0\">\n                    Mhm looks like you do not have any apps yet - <a @click=\"modeCreateApp()\">go create one!</a>\n                </p>\n                <div class=\"app-name d-flex justify-between align-center\" v-for=\"app in apps\" :key=\"app.uuid\">\n                    <span class=\"dots\">{{ app.name }}</span>\n                    <span class=\"icon icon-delete hover big\" @click=\"removeApp(app.uuid)\"></span>\n                </div>\n            </div>\n        </div>\n        <div>\n            <Tabs v-if=\"!isInCreateMode\" ref=\"Tabs\" :class=\"{ block: blockTabs }\" :initTab=\"'App Token'\" :tabs=\"[{name:'App Token'},{name:'Member'}]\" @tabChange=\"tabChange\"/>\n            <TabCreateApp v-if=\"isInCreateMode\" />\n            <TabAppToken v-if=\"activeTab === 'App Token' && !isInCreateMode\" @inEdit=\"enableDisableTabs\"/>\n            <TabMember v-if=\"activeTab === 'Member' && !isInCreateMode\" @inEdit=\"enableDisableTabs\" />\n        </div>\n    </div>\n</template>\n\n<script>\n    import Tabs from '@/components/utils/Tabs.vue';\n    import TabAppToken from '@/components/apps/TabAppToken.vue';\n    import TabCreateApp from '@/components/apps/TabCreateApp.vue';\n    import TabMember from '@/components/apps/TabMember.vue';\n    import axios from 'axios';\n\n    export default {\n        name: 'ViewApp',\n        components: {\n            Tabs,\n            TabCreateApp,\n            TabAppToken,\n            TabMember,\n        },\n        computed: {\n            blockTabs() {\n                return this.tabsBlocked;\n            }\n        },\n        data() {\n            return {\n                isInCreateMode: false, // change back to false once api works again. true just to check on the other views\n                activeTab: 'App Token',\n                tabsBlocked: false,\n                newApp: { name: '' },\n                apps: [],\n            };\n        },\n        mounted() {\n            this.getAppList().then(data => {\n                this.apps = data;\n                this.isInCreateMode = false;\n                // if (this.app !== null && this.apps.length === 0) this.isInCreateMode = true;\n            }).catch(error => {\n                // if apps can not be loaded display panel to create a new app\n                console.log(\"could not get apps\");\n                // this.isInCreateMode = true;\n            });\n            \n        },\n        props: ['status'],\n        methods: {\n            modeCreateApp() {\n                this.enableDisableTabs();\n                this.isInCreateMode = true;\n            },\n            async createNewApp() {\n                let options = {\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': localStorage.getItem(\"token\"),\n                    }\n                };\n                await axios.post(\"http://localhost:8080/api/v1/app/create\", {\n                        app_name: this.newApp.name,\n                    }, options\n                ).then((resp) => {\n                    console.log(\"Resp \",resp)\n                    // if (resp.status === 400) {\n                    //     this.$toast.warning(\"App name must be unique\")\n                    // }\n                }).catch(err => {\n                    this.$toast.warning(\"App name must be unique\");\n                    return;\n                });\n                const al = await this.getAppList();\n                this.apps = al;\n            },\n            async getAppList() {\n                let options = {\n                    headers: {\n                        'Authorization': localStorage.getItem(\"token\"),\n                    }\n                };\n                const res = await axios.get(\"http://localhost:8080/api/v1/app/getall\", options)\n                console.log(res);\n                if (res === null || res.status >= 400) this.isInCreateMode = true;\n                return res.data;\n                \n            },\n            async removeApp(id) {\n                let options = {\n                    headers: {\n                        'Content-Type': 'application/json',\n                        'Authorization': localStorage.getItem(\"token\"),\n                    }\n                };\n                await axios.post(\"http://localhost:8080/api/v1/app/delete\", {\n                        app_uuid: id,\n                    }, options\n                ).then((resp) => {\n                    console.log(\"Resp \",resp)\n                }).catch(err => {\n                    this.$toast.warning(\"Sorry app could not be removed\");\n                    return;\n                });\n                const al = await this.getAppList();\n                this.apps = al;\n            },\n            tabChange(tab) {\n                this.isInCreateMode = false;\n                this.activeTab = tab;\n            },\n            enableDisableTabs(toggle) {\n                this.tabsBlocked = toggle;\n                // block tab change by sending toggle to @Tabs\n                this.$refs.Tabs.block = this.tabsBlocked;\n            }\n        },\n    };\n</script>\n\n<style scoped>\n.tab-line {\n    grid-column: 1;\n    grid-row: 1;\n}\n\n.view_app {\n    display: grid;\n    grid-template-columns: 185px 1fr;\n    grid-column-gap: 15px;\n    height: 100%;\n}\n\n.app_list {\n    background: #fff;\n    border-radius: 8px;\n    padding: 15px;\n    height: max-content;\n    min-height: 225px;\n    max-height: 100%;\n    overflow-y: scroll;\n    border: 1px solid #ccc;\n}\n\n.app_name_list {\n    margin-top: 25px;\n}\n.app-name {\n    background:linear-gradient(135deg, #50e3c2 0%,#10d574 100%);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent; \n    font-size: 16px;\n    font-weight: bold;\n    padding: 5px;\n    margin: 5px 0;\n    border: 1px solid #10d574;\n    border-radius: 8px;\n    border-style: dashed;\n}\n.app-name:hover {\n    cursor: pointer;\n    text-decoration: underline;\n}\n\n</style>\n"],"sourceRoot":""}]}