e\">\n        <div :class=\"{active: activeTab===tab.name}\" v-for=\"tab in tabs\" :key=\"tab.name\" class=\"tab\" @click=\"setActive(tab.name)\">\n            <span class=\"text\">{{ tab.name }}</span>\n        </div>\n    </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'Tabs',\n  data() {\n      return {\n          activeTab: this.initTab,\n      };\n  },\n  props: {\n      initTab: String,\n      tabs: {\n          type: Array,\n          default() { return []; },\n      },\n  },\n  methods: {\n      setActive(clicked) {\n        this.activeTab = clicked;\n        this.$emit('tabChange', clicked);\n      }\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.tab-line {\n  display: flex;\n  justify-content: flex-start;\n  align-items: center;\n  grid-column-gap: 5px;\n}\n.tab {\n    background: #fff;\n    font-size: 14px;\n    font-weight: bolder;\n    border-radius: 8px;\n    padding: 5px 10px;\n}\n.tab:hover {\n    cursor: pointer;;\n}\n\n.active {\n    background: linear-gradient(135deg, #50e3c2 0%,#10d574 100%);\n}\n.active .text {\n    -webkit-text-fill-color: #fff; \n}\n\n.text {\n    background: linear-gradient(135deg, #50e3c2 0%,#10d574 100%);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n}\n</style>\n"],"sourceRoot":""}]}