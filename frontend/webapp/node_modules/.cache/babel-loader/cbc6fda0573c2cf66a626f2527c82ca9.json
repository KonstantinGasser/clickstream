             {id:1},\n                    {id:2},\n                    {id:3},\n                    {id:4},\n                    {id:5},\n                    {id:6},\n                    {id:7},\n                    {id:8},\n                    {id:9},\n                    {id:10},\n                    {id:11},\n                ],\n            };\n        },\n        computed: {\n            dummyMemberList() {\n                return this.dummyMember;\n            }\n        },\n        methods: {\n            setMode() {\n                this.isEdit = !this.isEdit;\n                // emit panel is in edit mode\n                // diable tabs until saved\n                this.$emit('inEdit', this.isEdit);\n            },\n            removeMember(id) {\n                this.dummyMember = this.dummyMember.filter(item => item.id !== id);\n            },\n            addMember() {\n                const id = this.dummyMember.length > 0 ? this.dummyMember[this.dummyMember.length -1].id+1 : 1;\n                this.dummyMember.push({id: id});\n            }\n        },\n    };\n</script>\n\n<style scoped>\n.view_component {\n    display: grid;\n    grid-template-rows: 50px 1fr;\n    grid-row-gap: 10px;\n}\n.user_grid {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));\n    grid-template-rows: repeat(auto-fill, minmax(200px, 1fr));\n    grid-gap: 10px;\n    overflow-y: scroll;\n    height: 100%;\n    max-height: 93vh;\n}\n\n.action {\n    background: #fff;\n    border-radius: 8px;\n    box-shadow: 0px 0px 5px 2px rgb(0,0,0, 0.1);\n    padding: 5px;\n}\n\n\nh2 {\n    margin: 5px 0px;\n}\n\n</style>\n"],"sourceRoot":""}]}